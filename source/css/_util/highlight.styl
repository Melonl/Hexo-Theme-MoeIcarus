.highlight
  margin: 0px
  display: block
  overflow-x: auto
  padding: 0 10px
  font-size: font-size - 1px
  font-family: font-mono
  line-height: font-size * line-height
  font-weight: 400
  background: #f8f8f8
  table
    margin: 0
    width: auto
    td
      border: none
    td.code
      padding-right: 20px
  .gutter
    pre
      color: #383a42
      text-align: right
      padding-right: 10px

.article-entry
  .highlight
    margin: 1.6em 0
    .line
      height: 1.6em;



@import "../_highlight/" + highlight
